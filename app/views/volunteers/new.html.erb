<div class="container">
  <div class ="row">
    <%= form_for @volunteer, :html => {:class => 'validate'} do |v| %>
      <div class="input-field col s6">
        <i class="material-icons prefix">account_circle</i>
        <%= v.text_field :first_name %>
        <%= v.label :first_name %>
      </div>
      <div class="input-field col s6">
        <%= v.text_field :last_name %>
        <%= v.label :last_name %>
      </div>
      <div class="input-field col s6">
        <i class="material-icons prefix">email</i>
        <%= v.email_field :email %>
        <%= v.label :email, "Email\*" %>
      </div>
      <div class="input-field col s6">
        <%= v.email_field :email %>
        <%= v.label :email, "Email \(retype\)\*" %>
      </div>
      <div class="input-field col s6">
        <i class="material-icons prefix">phone</i>
        <%= v.telephone_field :phone %>
        <%= v.label :phone, "Phone \(optional\)" %>
      </div>
      <div class="submit_button waves-effect waves-light btn right light-green">
        <%= v.submit "Save"%>
      </div>
    <% end %>
  </div>
</div>
